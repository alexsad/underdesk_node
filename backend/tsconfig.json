{
    "compilerOptions": {
        "target": "es5"
        ,"module": "commonjs"
        ,"moduleResolution": "node"
        ,"outDir": "./dist"
        ,"experimentalDecorators": true
        ,"noImplicitAny":true		
        ,"sourceMap": false        
		,"declaration": false
		,"removeComments":true
		,"noEmitHelpers":true 
        ,"importHelpers":true
		,"suppressImplicitAnyIndexErrors": true
        ,"baseUrl": "./"
        ,"paths": {
            "tslib" : ["node_modules/tslib@1.5.0/tslib.d.ts"]
        }
    },
    "exclude": [
        "node_modules"
		,"dist"
    ],
    "compileOnSave": false
}
