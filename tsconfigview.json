{
    "version": "1.5.0",
    "compilerOptions": {
        "target": "es5",
        "module": "amd",
        "declaration": false,
        "noImplicitAny": true,
        "removeComments": true,
        "noLib": false,
        "emitDecoratorMetadata": false,
        "sourceMap": false,
        "outDir": "deploy/public/js",
        "experimentalDecorators": true,
        "isolatedModules": false,
        "noEmitHelpers": true,
        "noResolve": true
    },
    "compileOnSave": false,
    "filesGlob": [
        "./src/br/**/view/*.ts",
        "./src/br/**/model/I[A-Z]*.ts",
        "!./node_modules/**/*.ts",
        "!./deploy/node_modules/**/*.ts",
        "./src/lib/underas/*.d.ts",
        "./src/lib/jquery/jquery2.d.ts",
        "./src/lib/jqueryui/jqueryui.d.ts"
    ],
    "reference": [],
    "files": [
        "src/br/underdesk/tabela/view/Tabela.ts",
        "src/br/underdesk/tabela/view/TabelaCampo.ts",
        "src/br/underdesk/tabela/model/ITabela.ts",
        "src/br/underdesk/tabela/model/ITabelaCampo.ts",
        "src/lib/underas/container.d.ts",
        "src/lib/underas/controller.d.ts",
        "src/lib/underas/core.d.ts",
        "src/lib/underas/net.d.ts",
        "src/lib/underas/util.d.ts",
        "src/lib/jquery/jquery2.d.ts",
        "src/lib/jqueryui/jqueryui.d.ts"
    ]
}